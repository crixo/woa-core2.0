@model Woa.Models.Consulto

@{
    ViewData["Title"] = "Details";
}

@section Paziente {
    @await Component.InvokeAsync("PazienteHeader", new { pazienteId = Model.PazienteId })
}

<h3>Dettagli Consulto</h3>

<div class="container-fluid">
    <div class="row">
        <div class="col-md-4">
            @await Component.InvokeAsync("Consulto", new { consultoId = Model.ID })
	         @await Component.InvokeAsync("AnamnesiProssima", new { consultoId = Model.ID })
        </div>
        <div class="col-md-8">
            @await Component.InvokeAsync("Esami", new { consultoId = Model.ID }) 
            @await Component.InvokeAsync("Trattamenti", new { consultoId = Model.ID }) 
            @await Component.InvokeAsync("Valutazioni", new { consultoId = Model.ID }) 
        </div>
    </div>
</div>







